{
  "name": "shrink-link",
  "version": "2.1.1",
  "license": "MIT",
  "author": "Sam Rose",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/samrose3/shrink-link.git"
  },
  "scripts": {
    "commit": "git-cz",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:js": "eslint --ignore-path .gitignore .",
    "lint:style": "stylelint src/**/*.css",
    "type-check": "tsc",
    "test": "jest",
    "build": "next build src && tsc && tsc --project tsconfig.server.json",
    "dev": "nodemon --inspect=0.0.0.0",
    "start": "NODE_ENV=production node src/.next/server",
    "storybook": "start-storybook",
    "storybook:build": "build-storybook",
    "deploy:book": "build-storybook && push-dir --dir=storybook-static --branch=gh-pages && rimraf storybook-static",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook",
      "pre-commit": "npm run lint",
      "pre-push": "npm run test"
    }
  },
  "dependencies": {
    "@types/express": "^4.17.1",
    "@types/morgan": "^1.7.37",
    "@types/styled-jsx": "^2.2.8",
    "@zeit/next-typescript": "^1.1.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "classnames": "^2.2.6",
    "clipboard": "^2.0.4",
    "consola": "^2.10.1",
    "date-fns": "^1.30.1",
    "dotenv": "^6.2.0",
    "express": "^4.17.1",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.15",
    "mongoose": "^5.7.5",
    "morgan": "^1.9.1",
    "next": "^8.1.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "snyk": "^1.290.1",
    "tachyons": "^4.11.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@storybook/react": "^5.2.4",
    "@types/jest": "^24.0.19",
    "@types/next": "^8.0.6",
    "@types/react": "^16.9.9",
    "@types/react-dom": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "@zeit/next-css": "^1.0.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-inline-react-svg": "^1.1.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "husky": "^1.3.1",
    "jest": "^24.9.0",
    "next-purgecss": "^3.1.1",
    "nodemon": "^1.19.4",
    "postcss-easy-import": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-reporter": "^6.0.1",
    "prettier": "^1.18.2",
    "push-dir": "^0.4.1",
    "react-testing-library": "^6.1.2",
    "rimraf": "^2.7.1",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "snyk": true
}
